{
    "name": "fcoedno/bolton",
    "description": "Backend challenge for backend engineer at Arquivei",
    "type": "project",
    "license": "mit",
    "require": {
        "php": "^7.4.1",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "ramsey/uuid": "^3.9",
        "symfony/console": "4.4.*",
        "symfony/dotenv": "4.4.*",
        "symfony/flex": "^1.3.1",
        "symfony/framework-bundle": "4.4.*",
        "symfony/orm-pack": "^1.0",
        "symfony/yaml": "4.4.*"
    },
    "require-dev": {
        "coduo/php-matcher": "^3.2",
        "phpstan/phpstan": "^0.12.3",
        "phpstan/phpstan-deprecation-rules": "^0.12.0",
        "phpstan/phpstan-doctrine": "^0.12.5",
        "phpstan/phpstan-phpunit": "^0.12.2",
        "phpstan/phpstan-symfony": "^0.12.1",
        "phpunit/phpunit": "^8.5",
        "sensiolabs-de/deptrac-shim": "^0.6.0",
        "squizlabs/php_codesniffer": "^3.5",
        "symfony/browser-kit": "4.4.*"
    },
    "config": {
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "Bolton\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bolton\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "4.4.*"
        }
    }
}
